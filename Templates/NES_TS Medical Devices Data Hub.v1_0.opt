        <archetype_id>
            <value>openEHR-EHR-CLUSTER.medical_device_regulatory_details.v0</value>
        </archetype_id>
        <term_definitions code="at0000">
            <items id="text">Medical device regulatory details</items>
            <items id="description">NES_TS UK_MHRA Medical Device. This cluster is for enabling the recording of regulatory details about a medical device, specifically 'class', in the EHR. THe requirement originally arose in the NHS Scotland Medical Devices Data Hub programme, part of the 'Scan for Safety' programme intended to enable the capture of details of implantable medical devices at the time of surgical procedures. This cluster allows the programme to capture the 'Class' value as it was at that event.</items>
        </term_definitions>
        <term_definitions code="at0001">
            <items id="text">Class</items>
            <items id="description">The Class of device as defined by the associated regulatory body or policy.</items>
            <items id="comment">For example, Class I</items>
        </term_definitions>
        <term_definitions code="at0003">
            <items id="text">Regulating organisation</items>
            <items id="description">The organisation that provides the values and definitions of the associated regulatory device metadata.</items>
            <items id="comment">For example: UK MHRA.</items>
        </term_definitions>
        <term_definitions code="at0005">
            <items id="text">Extension</items>
            <items id="description">Additional information about the regulatory data values.</items>
        </term_definitions>
        <term_definitions code="at0007">
            <items id="text">Category</items>
            <items id="description">The category of the device, as defined by the associated regulatory body or policy.</items>
            <items id="comment">For example, '1. Non Invasive'</items>
        </term_definitions>
        <term_definitions code="at0008">
            <items id="text">Other</items>
            <items id="description">Any additional allocated taxonomy values not elsewhere available.</items>
            <items id="comment">For example: 'Type', 'Branch', 'Chapter'.</items>
        </term_definitions>
    </children>
